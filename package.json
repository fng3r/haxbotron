{
  "name": "haxbotron",
  "version": "0.5.1",
  "description": "Haxbotron is a headless host server application for Haxball.",
  "author": "dapucita",
  "license": "MIT",
  "homepage": "https://github.com/dapucita/haxbotron",
  "bugs": {
    "url": "https://github.com/dapucita/haxbotron/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dapucita/haxbotron.git"
  },
  "scripts": {
    "install:packages": "npm run install:packages:core && npm run install:packages:db && npm run install:packages:web",
    "install:packages:core": "cd core && npm install",
    "install:packages:db": "cd db && npm install",
    "install:packages:web": "cd web && npm install",
    "build": "npm run build:core && npm run build:db && npm run build:web",
    "build:core": "cd core && npm run build",
    "build:db": "cd db && npm run build",
    "build:web": "cd web && npm run build",
    "start:core": "cd core && npm start",
    "start:db": "cd db && npm start",
    "start:web": "cd web && npm start",
    "dev:web": "cd web && npm run dev"
  },
  "dependencies": {}
}
